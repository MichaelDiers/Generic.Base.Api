{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Audience": "audience",
    "Issuer": "issuer",
    "KeyName": "GOOGLE_SECRET_MANAGER_JWT_KEY",
    "AccessTokenExpires": 30,
    "RefreshTokenExpires": 7200
  },
  "EnvNames": {
    "ApiKeyName": "X_API_KEY",
    "JwtKeyName": "GOOGLE_SECRET_MANAGER_JWT_KEY"
  },
  "DocumentationHealthCheck": {
    "DocumentationUrl": "https://michaeldiers.github.io/demos",
    "DocumentationDescription": "Access the documentation on github pages."
  },
  "ApiKey": {
    "KeyName": "X_API_KEY"
  },
  "HealthCheck": {
    "Route": "health"
  }

}
